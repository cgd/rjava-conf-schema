<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://jax.org/j-qtl"
            xmlns:tns="http://jax.org/j-qtl"
            elementFormDefault="qualified"
            xmlns:ns0="http://jax.org/r">
    <xsd:import schemaLocation="../../jax.org/r/r-application-state.xsd" namespace="http://jax.org/r"/>
    <xsd:element name="jQtlApplicationState">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension xmlns:ns0="http://jax.org/r" base="ns0:rApplicationStateType">
                    <xsd:sequence>
                        <xsd:element name="recentCrossDataDirectory" minOccurs="0" maxOccurs="1" type="ns0:fileType" xmlns:ns0="http://jax.org/r"/>
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
